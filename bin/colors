echo -e "\033[33mYELLOW\t\033[93mBRIGHT_YELLOW"
echo -e "\033[34mBLUE\t\033[94mBRIGHT_BLUE"
echo -e "\033[35mPURPLE\t\033[95mBRIGHT_PURPLE"
echo -e "\033[36mCYAN\t\033[96mBRIGHT_CYAN"
echo -e "\033[37mWHITE\t\033[97mBRIGHT_WHITE"
